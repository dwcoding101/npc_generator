{
  "name": "dev-test-env",
  "version": "1.0.0",
  "description": "Development environment for Typescript using Jasmine and Karma for testing ",
  "main": "index.js",
  "scripts": {
    "build-debug": "webpack --config=webpack.debug.config.js",
    "build-docs": "webpack --config=webpack.doc.config.js",
    "build": "webpack --config=webpack.config.js",
    "build-test": "webpack --config=webpack.spec.config.js",
    "exec": "node ./dist/main-bundle.js",
    "exec-test": "node ./src/test/testscript.js --testBundle=./dist/main-test-bundle.js"
  },
  "keywords": [],
  "author": "D.Whitney",
  "license": "ISC",
  "devDependencies": {
    "jasmine": "^3.1.0",
    "jasmine-console-reporter": "^3.0.0",
    "jasmine-expect": "^3.8.3",
    "ts-loader": "^4.1.0",
    "typedoc": "^0.11.1",
    "typedoc-webpack-plugin": "^1.1.4",
    "typescript": "^2.8.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "yargs": "^11.0.0"
  },
  "dependencies": {
    "@types/jasmine": "^2.8.6",
    "@types/node": "^9.6.2"
  }
}
